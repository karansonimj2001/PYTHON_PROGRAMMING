   "source": [
    "class circle:\n",
    "    def area(self,r):\n",
    "        print(3.14*r**2)\n",
    "    def perimeter(self,p):\n",
    "        print(2*3.14*(p))\n",
    "\n",
    "obj=circle()\n",
    "obj.area(5)\n",
    "obj.perimeter(7)"
